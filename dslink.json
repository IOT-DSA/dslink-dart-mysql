{
  "name": "dslink-dart-mysql",
  "version": "1.0.1",
  "description": "MySQL DSLink",
  "main": "bin/run.dart",
  "engines": {
    "dart": ">=1.13.0"
  },
  "getDependencies": [],
  "configs": {
    "broker": {
      "type": "url"
    },
    "name": {
      "type": "string",
      "default": "MySQL"
    },
    "token": {
      "type": "string"
    },
    "log": {
      "type": "enum[info,off,warning,severe,fine,finer,finest,all]",
      "default": "info"
    }
  }
}
